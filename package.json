{
  "name": "react-single-dropdown",
  "version": "1.0.5",
  "description": "Single select dropdown for react",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest --watch --verbose",
    "start": "webpack-dev-server --progress --colors",
    "build": "rm -rf lib && webpack --config webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanoop-jose/react-single-dropdown.git"
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "select",
    "select-box",
    "single-select-box",
    "single",
    "dropdown",
    "simple",
    "option"
  ],
  "author": "Sanoop Jose <snupjos@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanoop-jose/react-single-dropdown/issues"
  },
  "homepage": "https://github.com/sanoop-jose/react-single-dropdown#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.2",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^1.3.1",
    "regenerator-runtime": "^0.10.5",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/mocks",
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
